<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:validation="http://www.eclipse.org/sirius/description/validation/1.1.0" name="maquette" version="12.0.0.2017041100">
  <ownedViewpoints name="MyViewpoint">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="diagramFormationUniv" domainClass="maquette.Formations" enablePopupBars="true">
      <validationSet>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="Formation" label="Formation" level="ERROR" message="Formations Validation" targetClass="maquette.Formation">
          <audits auditExpression="[semestres->size() > 1/]"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="Compétence" label="Compétence" level="ERROR" targetClass="maquette.Compétence">
          <audits auditExpression="[ues->size() > 0/]"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="Semestre" label="Semestre" level="ERROR" message="Semestre" targetClass="maquette.Semestre">
          <audits auditExpression="[nbECTS >= 30/]"/>
        </ownedRules>
      </validationSet>
      <defaultLayer name="Default">
        <nodeMappings name="UENode" label="UE" semanticCandidatesExpression="[self.formations->collect(uesPotentielles)/]" domainClass="maquette.UE">
          <style xsi:type="style:SquareDescription" labelSize="12" showIcon="false" labelExpression="[self.nom/]" sizeComputationExpression="[2*self.nbECTS/]" labelPosition="node">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
          </style>
          <conditionnalStyles predicateExpression="[not self.disciplinaire/]">
            <style xsi:type="style:SquareDescription" labelSize="12" showIcon="false" labelExpression="[self.nom/]" sizeComputationExpression="[2*self.nbECTS/]" labelPosition="node">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </conditionnalStyles>
        </nodeMappings>
        <nodeMappings name="FormationNode" label="Formation" domainClass="maquette.Formation">
          <style xsi:type="style:SquareDescription" labelSize="12" showIcon="false" labelExpression="[self.nom/]" labelPosition="node" width="15" height="8">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_chocolate']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="SemestreNode" label="Semestre" domainClass="maquette.Semestre">
          <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="[self.numero/]" sizeComputationExpression="[2*self.nb ECTS/]" labelPosition="node" width="10" height="10">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_yellow']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="SemestresUEs" sourceMapping="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='diagramFormationUniv']/@defaultLayer/@nodeMappings[name='SemestreNode']" targetMapping="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='diagramFormationUniv']/@defaultLayer/@nodeMappings[name='UENode']" targetFinderExpression="[self.ues/]">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="DefaultTool" label="DefaultLabel">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="createFormation" label="AddFormation" forceRefresh="true" nodeMappings="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='diagramFormationUniv']/@defaultLayer/@nodeMappings[name='FormationNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="maquette::Formation" referenceName="formations"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="createUESemestre" label="AddUESemestre" edgeMappings="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='diagramFormationUniv']/@additionalLayers[name='Competences']/@edgeMappings[name='UEsCompétences']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:source">
                <subModelOperations xsi:type="tool_1:SetValue" featureName="ues" valueExpression="var:target"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="createUECompetence" label="AddUECompetence" forceRefresh="true" edgeMappings="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='diagramFormationUniv']/@additionalLayers[name='Competences']/@edgeMappings[name='UEsCompétences']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:source">
                <subModelOperations xsi:type="tool_1:SetValue" featureName="compétence" valueExpression="var:target"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
      <additionalLayers name="Competences" activeByDefault="true">
        <nodeMappings name="CompétenceNode" domainClass="maquette.Compétence">
          <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="[self.nom/]" labelPosition="node" width="10" height="10">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_green']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="UEsCompétences" sourceMapping="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='diagramFormationUniv']/@defaultLayer/@nodeMappings[name='UENode']" targetMapping="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='diagramFormationUniv']/@additionalLayers[name='Competences']/@nodeMappings[name='CompétenceNode']" targetFinderExpression="[self.compétence/]">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
      </additionalLayers>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="maquette.design.Services"/>
  </ownedViewpoints>
</description:Group>
